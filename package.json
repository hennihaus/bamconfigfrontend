{
  "name": "bamconfigfrontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite build --mode=developement && vite preview",
    "build": "run-p type-check build-only",
    "build-only": "vite build --mode=production",
    "type-check": "vue-tsc --noEmit --strict",
    "lint": "npm run lint:ci -- --fix",
    "lint:ci": "eslint ./src --no-eslintrc --config .eslintrc.cjs --max-warnings 0 --exit-on-fatal-error",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^36.0.1",
    "@ckeditor/ckeditor5-vue": "^4.0.1",
    "@hennihaus/bamconfigbackend": "^10.0.2",
    "@vee-validate/i18n": "^4.8.3",
    "@vee-validate/rules": "^4.8.4",
    "axios": "^1.3.4",
    "qs": "^6.11.1",
    "semantic-ui-css": "^2.5.0",
    "uuid": "^9.0.0",
    "vee-validate": "^4.8.4",
    "vue": "^3.2.47",
    "vue-dompurify-html": "^4.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.2.0"
  }
}
